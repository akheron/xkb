default partial alphanumeric_keys
xkb_symbols "akheron" {
    include "akheron(fi_cmk_ed_ks)"
    include "colemak(cmk_ed_dh)"
    include "level3(caps_switch)"
    include "akheron(alt_lwin)"
    include "akheron(lv5_lalt_switch)"
    include "group(switch)"
    include "akheron(caps_b)"
    include "akheron(lv5)"
    include "akheron(group2)"
    name[Group1] = "Finnish (Colemak[akheron])";
};

partial alphanumeric_keys
xkb_symbols "fi_cmk_ed_ks" {
    include "fi(kotoistus)"
    include "colemak(cmk_ed_letters)"

    key <AD10> { [odiaeresis, Odiaeresis, oslash, Ooblique ] };
};

partial modifier_keys
xkb_symbols "alt_lwin" {
    key <LWIN> { [ Alt_L ] };
    modifier_map Mod1 { <LWIN> };
};

partial modifier_keys
xkb_symbols "lv5_lalt_switch" {
    include "level5(modifier_mapping)"
    key <LALT> {
        type[Group1] = "ONE_LEVEL",
        symbols[Group1] = [ ISO_Level5_Shift ]
    };
};

partial modifier_keys
xkb_symbols "caps_b" {
    key <LSGT> {
        type[Group1] = "ONE_LEVEL",
        symbols[Group1] = [ Caps_Lock ]
    };
};

